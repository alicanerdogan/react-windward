{
  "name": "eslint-plugin-react-css-cx",
  "version": "0.0.0-development",
  "description": "eslint plugin for react-css-cx",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*",
    "custom_types"
  ],
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn minify",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "minify": "yarn minify:esm && yarn minify:cjs",
    "minify:esm": "terser --compress --mangle --module -o lib/esm/index.mjs -- lib/esm/index.mjs ",
    "minify:cjs": "terser --compress --mangle -o lib/cjs/index.js -- lib/cjs/index.js",
    "test": "jest",
    "semantic-release": "semantic-release",
    "prepack": "yarn build",
    "lint": "eslint \"src/**/*.ts\"",
    "format:all": "prettier --write \"src/**/*.ts\""
  },
  "release": {
    "branches": [
      "main"
    ],
    "extends": "semantic-release-monorepo"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alicanerdogan/react-css-cx.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "react-css-cx",
    "tailwind",
    "react",
    "css",
    "typescript",
    "esm",
    "cjs",
    "css-in-js",
    "commonjs",
    "ecmascript"
  ],
  "author": "Alican Erdogan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alicanerdogan/react-css-cx/issues"
  },
  "homepage": "https://github.com/alicanerdogan/react-css-cx#readme",
  "engines": {
    "node": ">= 16"
  },
  "dependencies": {},
  "peerDependencies": {
    "eslint": ">= 5"
  },
  "devDependencies": {
    "@types/eslint": "^8.4.6",
    "@types/estree": "^1.0.0",
    "@types/jest": "^29.2.5",
    "@types/node": "^18.11.18",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-eslint-plugin": "^5.0.7",
    "jest": "^29.3.1",
    "prettier": "^2.1.2",
    "semantic-release": "^20.0.2",
    "semantic-release-monorepo": "^7.0.5",
    "terser": "^5.16.1",
    "ts-jest": "^29.0.5",
    "typescript": "^4.9.4"
  }
}
