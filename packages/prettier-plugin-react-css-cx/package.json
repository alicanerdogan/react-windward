{
  "name": "prettier-plugin-react-css-cx",
  "version": "0.0.0-development",
  "description": "A Prettier plugin to format react-css-cx classes",
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "workspaces": {
    "prettier-plugin-fake": "./prettier-plugin-fake"
  },
  "scripts": {
    "clean": "rm -rf ./lib",
    "build": "yarn clean && yarn build:esm && yarn build:cjs && yarn minify",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && mv lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "minify": "yarn minify:esm && yarn minify:cjs",
    "minify:esm": "terser --compress --mangle --module -o lib/esm/index.mjs -- lib/esm/index.mjs ",
    "minify:cjs": "terser --compress --mangle -o lib/cjs/index.js -- lib/cjs/index.js",
    "test": "jest",
    "semantic-release": "semantic-release",
    "prepack": "yarn build",
    "lint": "eslint --ext '.ts' ./src",
    "prettier:all": "prettier --write \"**/*.ts\""
  },
  "release": {
    "branches": [
      "main"
    ],
    "extends": "semantic-release-monorepo"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/alicanerdogan/react-css-cx.git"
  },
  "keywords": [
    "prettier",
    "plugin",
    "react",
    "css",
    "typescript",
    "esm",
    "cjs",
    "css-in-js",
    "commonjs",
    "ecmascript"
  ],
  "author": "Alican Erdogan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/alicanerdogan/react-css-cx/issues"
  },
  "homepage": "https://github.com/alicanerdogan/react-css-cx#readme",
  "devDependencies": {
    "@types/jest": "^29.2.5",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "eslint": "^8.31.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "semantic-release": "^20.0.2",
    "terser": "^5.16.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "peerDependencies": {
    "prettier": ">=2.1.2"
  },
  "dependencies": {
    "@babel/generator": "^7.20.7",
    "@babel/parser": "^7.20.7",
    "@babel/types": "^7.20.7"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
